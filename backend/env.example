# CLIFF Backend Environment Example
# Copy this file to .env (inside backend folder) and set values

ENVIRONMENT=development
DEBUG=true
BACKEND_HOST=0.0.0.0
BACKEND_PORT=8000

# NASA
NASA_API_KEY=DEMO_KEY
# Optional overrides
# NASA_BASE_URL=https://api.nasa.gov
# NASA_NEOWS_URL=https://api.nasa.gov/neo/rest/v1

# Database (development-friendly)
MONGODB_URL=mongodb://localhost:27017/cliff_db
MONGODB_NAME=cliff_db

# Redis (optional)
REDIS_URL=redis://localhost:6379/0

# Security
JWT_SECRET=change-me-please-change-me-please-change-me
JWT_ALGORITHM=HS256

# Frontend
FRONTEND_URL=http://localhost:3000
