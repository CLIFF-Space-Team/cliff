{
  "name": "cliff-nasa-app",
  "version": "1.1.0",
  "description": "CLIFF - Cosmic Level Intelligent Forecast Framework for NASA Space Apps Challenge 2025",
  "private": true,
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && python main.py",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:frontend": "cd frontend && npm run start",
    "start:backend": "cd backend && python main.py",
    "install:all": "npm install && npm run install:frontend",
    "install:frontend": "cd frontend && npm install",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "deploy:production": "npm run build && docker-compose -f docker-compose.prod.yml up -d"
  },
  "keywords": [
    "nasa",
    "space",
    "ai",
    "asteroid",
    "threat",
    "monitoring",
    "nextjs",
    "fastapi",
    "threejs",
    "websocket"
  ],
  "author": {
    "name": "kynuxdev",
    "email": "admin@kynux.dev",
    "url": "cliff.kynux.dev"
  },
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "react-error-boundary": "^6.0.0"
  }
}
